{
  "name": "New Tab",
  "description": "Override the new tab page with a blank one",
  "version": "0.2",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "http://www.google.com/",
    "https://query.yahooapis.com/",
    "https://clients1.google.com"
  ],
  //"content_security_policy": "script-src 'self', 'unsafe-eval', https://clients1.google.com; object-src 'self'",
  "content_security_policy": "script-src 'self' https://clients1.google.com; object-src 'self'",
  
  "manifest_version": 2
}
